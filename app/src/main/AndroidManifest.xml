<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="th.ac.kku.smartkidney">

    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".AddAlarmActivity">
        </activity>
        <activity android:name=".HealtEdActivity"/>
        <activity android:name=".HealthFormActivity"/>
        <activity
                android:name=".HomeActivity"
                android:noHistory="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity
                android:name=".AddFormActivity"
                android:theme="@style/ActivityTheme"/>
        <activity
                android:name=".RegisterActivity"
                android:theme="@style/ActivityTheme"/>
        <activity android:name=".LoginActivity">

        </activity>
        <activity android:name=".SplashActivity"/>
        <activity android:name=".SuggestActivity"/>
        <activity android:name=".AnalyzeActivity"/>
        <activity android:name=".RiskFormActivity"/>
        <activity android:name=".AgreementActivity"/>
        <activity android:name=".AlarmActivity"/>
    </application>

</manifest>